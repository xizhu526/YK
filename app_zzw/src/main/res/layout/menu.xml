    android:layout_height="match_parent"
    android:orientation="vertical"
    >
    <Button
        android:id="@+id/idBtnMyInfo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="个人信息"
        />
    <Button
        android:id="@+id/idBtnMyPublic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="我发布的"
        />
    <Button
        android:id="@+id/idBtnMyService"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="我服务的"
        />
</LinearLayout>